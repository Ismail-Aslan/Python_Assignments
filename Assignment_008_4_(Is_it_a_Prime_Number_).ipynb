      "source": [
        "nmb = int(input(\"Please enter a number : \"))\n",
        "if nmb > 1:\n",
        "  for i in range(2,nmb):\n",
        "    if nmb % i == 0:\n",
        "      print(nmb, \"is not a prime number.\")\n",
        "      break\n",
        "  else:\n",
        "    print(nmb, \"is a prime number.\")\n",
        "else:\n",
        "  print(nmb, \"is not a prime number.\")"
      ],
